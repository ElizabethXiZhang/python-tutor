### correct answer 
import string

def swap_case(s):
    output = []
    for letter in s:
        if letter in string.ascii_lowercase:
            output.append(letter.upper())
        elif letter in string.ascii_uppercase:
            output.append(letter.lower())
        else:
            output.append(letter)
    return "".join(output)

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
